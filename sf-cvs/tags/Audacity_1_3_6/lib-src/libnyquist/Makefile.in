CMTHDRS = cmt/seqdecls.h nyqsrc/seqext.h cmt/seq.h nyqsrc/seqinterf.h  \
	cmt/seqread.h cmt/seqmread.h cmt/seqwrite.h cmt/seqmwrite.h 

EVERYTHING = libnyquist.a

CURRENT = $(EVERYTHING)

current: $(CURRENT)

libnyquist.a: $(OBJECTS)
	ar ruv libnyquist.a $(OBJECTS)
	ranlib libnyquist.a

